{
  "name": "ecommerce-mern",
  "version": "1.0.0",
  "description": "MERN Shoppin Cart App",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/egluna-dev/ecommerce-mern.git"
  },
  "keywords": [
    "ecommerce",
    "shopping",
    "react",
    "paypal",
    "money",
    "tech",
    "react",
    "web",
    "app"
  ],
  "author": "E. Giovanni Luna",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/egluna-dev/ecommerce-mern/issues"
  },
  "homepage": "https://github.com/egluna-dev/ecommerce-mern#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.6.1"
  },
  "devDependencies": {
    "concurrently": "^7.4.0",
    "nodemon": "^2.0.20"
  }
